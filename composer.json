{
    "name": "protecms/cms",
    "description": "Software de gestiÃ³n para asociaciones protectoras de animales.",
    "keywords": ["software", "gestion", "asociaciones", "protectoras", "refugios", "animales", "cms", "laravel"],
    "license": "GPL-3.0+",
    "type": "project",
    "authors": [
        {
            "name": "Jaime Sares",
            "email": "jaimesares@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "dimsav/laravel-translatable": "^6.0",
        "igaster/laravel-theme": "^2.0",
        "intervention/image": "^2.4",
        "unisharp/laravel-filemanager": "^1.8",
        "guzzlehttp/guzzle": "^6.3",
        "spatie/laravel-analytics": "^3.3",
        "doctrine/dbal": "^2.6",
        "mews/captcha": "^2.1",
        "laravel/tinker": "^1.0",
        "laravel/dusk": "^2.0",
        "laravel/browser-kit-testing": "^1.0",
        "spatie/laravel-sitemap": "^3.3",
        "barryvdh/laravel-dompdf": "^0.8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-debugbar": "^2.2",
        "barryvdh/laravel-ide-helper": "^2.4",
        "squizlabs/php_codesniffer": "2.*",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
